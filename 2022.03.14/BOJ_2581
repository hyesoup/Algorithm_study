import math
m = int(input())
n = int(input())
array = []

def is_prime_num(x):
    if x == 1:
        return 0
    for i in range(2, int(math.sqrt(x)+1)):
        if x%i == 0:
            return 0
    return array.append(x)

for j in range(m, n+1):
    is_prime_num(j)


if len(array) == 0:
    print(-1)
else:
    print(sum(array))
    print(array[0])
